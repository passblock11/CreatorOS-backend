{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "crons": [
    {
      "path": "/api/posts/cron/publish-scheduled",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/posts/cron/sync-analytics",
      "schedule": "0 */6 * * *"
    }
  ]
}
